<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <title>Product Overview</title>
    <link rel="stylesheet" type="text/css" href="css/header.css">
    <link rel="stylesheet" type="text/css" href="css/products.css">
    <link rel="stylesheet" type="text/css" href="//fonts.googleapis.com/css?family=Rock+Salt" />
</head>

<body>
    <div class="products">
        <div class="productTitle">
            <h1>Products</h1>
        </div>
        <div class="product-grid">
            <% productNames.forEach( productTitle =>  { %>
            <div>
                <h2> <%= productTitle.title %></h2>
                <div class="imgBorder">
                <a href="/product/<%= productTitle.title %>"><img class="product-image" src="images/<%= productTitle.title %>.png"></a>
                <a href="/pathToAddToWishlist"><img class="wishlistIcon" src="images/wishlist.png"></a>
            </div>
            </div>
            <% }); %>
        </div>
    </div>
</body>

</html>